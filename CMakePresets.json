{
  "version": 2,
  "cmakeMinimumRequired": {
      "major": 3,
      "minor": 22,
      "patch": 0
  },
  "configurePresets": [
      {
          "name": "blinky",
          "displayName": "blinky",
          "description": "Blinky CM0",
          "generator": "Ninja",
          "environment": {
              "ARM_GCC_ROOT_FOLDER": "/Applications/ARM"
          },
          "binaryDir": "${sourceDir}/build/${presetName}",
          "cacheVariables": {
              "FETCHCONTENT_UPDATES_DISCONNECTED": "OFF",
              "CMAKE_BUILD_TYPE": "Debug",
              "CMAKE_INSTALL_PREFIX": "${sourceDir}/install/${presetName}",
              "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/build/${presetName}/_deps/platform_cmake-src/armgcc-cortex-m7.cmake",
              "GITHUB_BRANCH_TOOLCHAIN": "PLAT-1071"
          }
      }
  ],
  "buildPresets": [
      {
          "name": "blinky",
          "configurePreset": "blinky"
      }
  ]
}
